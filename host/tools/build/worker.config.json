{
  "description": {
    "language": "dotnet-isolated",
    "extensions": [ ".dll" ],
    "defaultExecutablePath": "%FUNCTIONS_WORKER_DIRECTORY%/bin/x64/FunctionsNetHost.exe",
    "defaultWorkerPath": "bin/x64/FunctionsNetHost.exe",
    "workerIndexing": "true"
  },
  "profiles": [
    {
      "profileName": "DotnetIsolatedLinux",
      "conditions": [
        {
          "conditionType": "hostProperty",
          "conditionName": "platform",
          "conditionExpression": "LINUX"
        }
      ],
      "description": {
        "defaultExecutablePath": "%FUNCTIONS_WORKER_DIRECTORY%/bin/FunctionsNetHost",
        "defaultWorkerPath": "bin/FunctionsNetHost"
      }
    },
    {
      "profileName": "DotnetIsolatedWinX86",
      "conditions": [
        {
          "conditionType": "hostProperty",
          "conditionName": "platform",
          "conditionExpression": "WINDOWS"
        },
        {
          "conditionType": "environment",
          "conditionName": "SITE_BITNESS",
          "conditionExpression": "x86"
        }
      ],
      "description": {
        "defaultExecutablePath": "%FUNCTIONS_WORKER_DIRECTORY%/bin/x86/FunctionsNetHost.exe",
        "defaultWorkerPath": "bin/x86/FunctionsNetHost.exe"
      }
    }
  ]
}